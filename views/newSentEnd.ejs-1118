<style type="text/css">
html, body {
  width: 100%;
  height: 100%;
  margin: 0;
}
p {
	margin: 0;
	padding: 0;
}

#sidebar, #content {
  height: 100%;
  overflow:visible;
  float:left;
  transition: width .35s;
}
#sidebar {
  background:white;
  width: 0%;
  margin: 2vw 2vw 0 2vw;
}

#sidebar.collapsed {
  width: 0;
}

#sidebar.collapsed + #content {
  width: 100%;
}

#content {
  width: 90%;
  margin: 2vw 0 0 2vw;
}
.sidebarBtn {
	font-size: 1.5vw;
	width: 100%;
	margin-bottom: 10px;
	padding: 6px 0 6px 0;
	background-color: #D6D6D6;
	border-style: none;
	border-radius: 5px;
}
.sidebarBtn:hover {
	cursor: pointer;
	background-color: #ACACAC;
}
.sidebarBtn:focus {
	outline: 0;
	background-color: #ACACAC;
}
.title {
	font-size: 1.5vw;
	padding-left: 10px;
	padding-bottom: 5px;
}
.hide {
	display: none;
}
.floatL {
	float: left;
}
.searchbar {
    width: 100%;
    text-align: center;
}
.sb1 {
    border-style: solid;
    border-width: 1px;
    border-radius: 5px;
    width: 30%;
    text-align: center;
    display: inline-block;
    height: 1.8vw;
    background-color: white;
}
.sb2 {
	width: 85%;
    font-size: 1.3vw;
    border-style: none;
}
.sb2:focus {
    outline: 0;
}
.f1 {
	font-size: 1.3vw;
	padding-right: 5%;
}
.header {
	width: 100%;
	padding: 8px 0 8px 0;
	border-style: none;
	background-color: #D6D6D6;
}
.downBtn {
	vertical-align: middle;
	font-size: 1.5vw;
	cursor: pointer;
}
.cbox {
	padding: 1% 2% 1% 2%;
	font-size: 1.3vw;
}
.nm {
	margin: 0;
	font-size: 1.1vw;
}
.tn {
	font-size: 0.01px;
	color: transparent;
	position: absolute;
	border-style: none;
	background-color: transparent;
	cursor: default;
}
.tn:focus {
	outline: 0;
}
.modal {
  display: none; 
  position: fixed; 
  z-index: 1; 
  padding-top: 100px; 
  left: 0;
  top: 0;
  width: 100%; 
  height: 100%; 
  overflow: auto; 
  background-color: rgb(0,0,0); 
  background-color: rgba(0,0,0,0.4); 
}
.modal1 {
    text-align: center;
}
.modal-content1 {
  border-radius: 10px;
  border-color: black;
  border-style: solid;
  border-width: 1px;
  background-color: white;
  margin: auto;
  margin-top: 125px;
  padding: 20px;
  width: 25%;
}
.pbtm {
	padding-bottom: 30px;
}
.card {
	width: 40%;
	padding: 0 1.5vw 0 1.5vw;
	margin: 10px 0 0 40%;
	display: none;
}
button {
  width: 30px;
  height: 30px;
  background: black;
}
</style>
